<UserControl x:Class="CarDealer.UserControls.OrdersControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CarDealer.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="500">
    <Border BorderThickness="1" Margin="10" BorderBrush="#05ace2">
        <Grid Background="#221e1f">
            <StackPanel>
                <Label x:Name="Buyer_Label"/>
                <Label x:Name="Mark_Label"/>
                <Label x:Name="Model_Label"/>
                <Label x:Name="EquipmentName_Label"/>
            </StackPanel>
        </Grid>
    </Border>
</UserControl>
